{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "auth": {
      "port": 1111
    },
    "firestore": {
      "port": 1112
    },
    "hosting": {
      "port": 1113
    },
    "ui": {
      "enabled": true,
      "port": 1110
    }
  }
}
